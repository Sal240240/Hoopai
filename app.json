{
  "expo": {
    "name": "Hoop AI",
    "slug": "basketball-workout-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#7851a9"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hoopai.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#7851a9"
      },
      "package": "com.hoopai.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "hoopai"
  }
}
